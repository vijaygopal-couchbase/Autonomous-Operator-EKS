{
    "logging": {
        "log_file_path": "/var/tmp/sglogs",
        "console": {
            "enabled": true,
            "log_level": "info",
            "log_keys": [
                "*"
            ]
        }
    },
    "databases": {
        "db": {
            "server": "couchbases://ual-cbs-srv",
